<!DOCTYPE html>
<html lang="fr">

	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		<title>
			Quai Antique -
			{% block title %}{% endblock %}
		</title>

		<!-- ======= Styles ====== -->

		<!-- Bootstrap CSS -->
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

		<!-- font awsome CSS -->
		{# <link rel="icon" type="image/png" sizes="32x32" href="assets/images/favicon-32x32.png"> #}
		<link
		rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css" integrity="sha512-H/K0xqyC9HhrN/M+dP25OPnb+Zx8cZJt/fZVtOUvO+zkOrI0GQj+hYsnSFr+ir9LsIyo8b4ot0Jvh4/P6FJh6w==" crossorigin="anonymous"> <!-- Feuilles de styles -->
		<link rel="stylesheet" href="{{ asset('assets/css/bootstrap.min.css') }}"> {% block stylesheets %}{% endblock %}
		<link rel="stylesheet" href="{{ asset('assets/css/styleUser.css') }}">
	</head>

	<body>


		<!-- Header des pages : navBar -->
		<nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top">
{% include "_partials/_nav.html.twig" %}
			
		</nav>

		<!-- Body des pages : contenu -->
		<div class="container-fluid"> {% block body %}
		{% include "_partials/_footer.html.twig" %}
		{% endblock %}
			</div>

			<!-- Footer des pages -->
			<footer class="bd-footer py-3 py-md-5 mt-5 bg-dark">
				{{ render(controller('App\\Controller\\HoraireController::getHoraire')) }}
			</footer>

			<!-- Fichiers JS -->
			<script src="{{ asset('assets/js/bootstrap.bundle.min.js') }}" defer></script>

			{% block scripts %}
				<!-- jQuery first, then Popper.js, then Bootstrap JS -->
				<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
				<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
				<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
			<!-- ====== ionicons ======= -->
			<script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
			<script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
			{% endblock %}
		</body>
	</html>
